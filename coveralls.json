{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "defexception",
    "defprotocol",
    "defstruct",
    "defdelegate",
    "def.+(.+\\\\.+).+do",
    "^\\s+use\\s+"
  ],
  "custom_stop_words": [
    "defwrap.+",
    ":erlang.unlink"
  ],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true
  },
  "skip_files": [
    "lib/cachex/macros.ex",
    "lib/cachex/macros/*",
    "lib/mix/cachex.ex",
    "lib/mix/tasks/*",
  ]
}
